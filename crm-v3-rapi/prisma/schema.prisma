/// Stages within prospect pipelines with conversion tracking
model ProspectPipelineStage {
  id                                                            String                  @id @default(uuid()) @db.Uuid()
  /// Integer value determining stage sequence in the pipeline. Lower values display first.
  order                                                         Int

/// Pre-opportunity leads with qualification scoring and temperature tracking
model Prospect {
  id                              String                          @id @default(uuid()) @db.Uuid()
  /// Enum field recording why a prospect was removed from active pipeline for analytics and process improvement.
  disqualificationReason          ProspectDisqualificationReason?
  /// UUID reference for tracking lead generation attribution. NOTE: This field needs to be linked as a foreign key to Marketing microservice Campaign model when it becomes available. externalRef
  sourceCampaign                  String?                         @db.Uuid()
  /// Free-text field capturing the prospect's expressed interests, pain points, and requirements.
  interestSummary                 String?
  /// External foreign key to HR microservice Employee model identifying the sales representative responsible for this prospect.
  ownerId                         String                          @db.Uuid()
  /// Foreign key to ProspectCategory for classifying prospects by type or source channel.
  categoryId                      String                          @db.Uuid()
  /// Foreign key to ProspectCategory for classifying prospects by type or source channel.
  category                        ProspectCategory                @relation("ProspectProspectCategoryCategoryId", fields: [categoryId], references: [id])
  /// Foreign key reference to the Person model representing the individual contact associated with this prospect.
  personId                        String                          @db.Uuid()
  /// Foreign key reference to the Person model representing the individual contact associated with this prospect.
  person                          Person                          @relation("ProspectPersonPersonId", fields: [personId], references: [id])
  /// Numeric score (0-100) indicating prospect qualification level based on fit, interest, and buying signals.
  qualificationScore              Int
  /// Enum field indicating prospect engagement level: COLD (dormant), WARM (engaged), HOT (ready to buy).
  temperature                     ProspectTemperature
  /// Foreign key to ProspectPipeline defining which qualification workflow this prospect follows.
  prospectPipelineId              String?                         @db.Uuid()
  /// Foreign key to ProspectPipeline defining which qualification workflow this prospect follows.
  prospectPipeline                ProspectPipeline?               @relation("ProspectProspectPipelineProspectPipelineId", fields: [prospectPipelineId], references: [id])
  /// Foreign key to ProspectPipelineStage indicating current position in the prospect qualification workflow.
  statusId                        String?                         @db.Uuid()
  /// Foreign key to ProspectPipelineStage indicating current position in the prospect qualification workflow.
  status                          ProspectPipelineStage?          @relation("ProspectProspectPipelineStageStatusId", fields: [statusId], references: [id])
  prospectProspectProductProspect ProspectProduct[]               @relation("ProspectProductProspectProspectId")
  color                           String?                         @db.VarChar(40)
  tags                            String? // Optional tags for easier categorization or search.
  everyoneCanSeeIt                Boolean                         @default(false) // Visibility control.
  anonymousCanSeeIt               Boolean                         @default(false) // Visibility control for anonymous users.
  everyoneInObjectCompanyCanSeeIt Boolean                         @default(true) // Company-wide visibility.
  onlyTheseRolesCanSeeIt          Json? // Specific roles with visibility access.
  onlyTheseUsersCanSeeIt          Json? // Specific users with visibility access.
  client                          String                          @db.Uuid() // Identifier for the client associated.
  createdBy                       String                          @db.Uuid() // User ID of the creator.
  updatedBy                       String                          @db.Uuid() // User ID of the last updater.
  createdAt                       DateTime                        @default(now()) // Creation timestamp.
  updatedAt                       DateTime                        @updatedAt // Last update timestamp.
  deleted                         DateTime? // Deletion timestamp, if deleted.
  workflowId                      String?                         @db.Uuid()
  workflowInstanceId              String?                         @db.Uuid()
  isSystemTemplate                Boolean                         @default(false)

  @@index([everyoneCanSeeIt, anonymousCanSeeIt, everyoneInObjectCompanyCanSeeIt, onlyTheseRolesCanSeeIt, onlyTheseUsersCanSeeIt, client, createdBy, isSystemTemplate, deleted, createdAt, updatedAt])
}

